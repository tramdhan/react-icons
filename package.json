{
  "name": "@tramdhan/react-icons",
  "version": "1.0.0",
  "description": "React SVG icon library",
  "main": "index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf src && rimraf dist",
    "build": "yarn run svgr && rollup -c",
    "svgr": "svgr --icon --title-prop --replace-attr-values '#2b4ea2=newColor' -d src assets"
  },
  "keywords": [
    "react",
    "svg",
    "icon",
    "icons",
    "iconography"
  ],
  "author": "Trevor Ramdhan",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@svgr/cli": "^6.3.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "rollup-plugin-filesize": "^9.1.2"
  }
}
